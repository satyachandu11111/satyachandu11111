<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="jquery/fileUploader/css/jquery.fileupload-ui.css"/>
    </head>
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Bss\ProductImagesByCustomer\Block\Adminhtml\Image\Edit" name="image_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Bss\ProductImagesByCustomer\Block\Adminhtml\Image\Edit\Tabs" name="image_edit_tabs">
                <block class="Bss\ProductImagesByCustomer\Block\Adminhtml\Image\Edit\Tab\Main" name="image_edit_tab_main"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">main_section</argument>
                    <argument name="block" xsi:type="string">image_edit_tab_main</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
